{
  "expo": {
    "name": "medium",
    "slug": "medium",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "medium",
    "userInterfaceStyle": "automatic",
    "jsEngine": "hermes",
    "updates": {
      "enabled": false
    },
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE"
      },
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ],
      "usesCleartextTraffic": true,
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.prach14.medium"
    },
    "web": {
      "output": "static"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d7557be1-da51-43ec-9fc0-ab578d101b98"
      }
    }
  }
}
